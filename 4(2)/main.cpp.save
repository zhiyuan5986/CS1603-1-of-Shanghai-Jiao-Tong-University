#include <iostream>
using namespace std;

int main()
{
    int n, m, sum=0;
    cin >> n;
    for (int j=0;j<10;++j) {
        m=n;
        sum=0;
        while (m!=0) {
            n=m%10;
            m/=10;
            sum += n;
        }
        n=sum;
    }
    cout << sum;
    return 0;
}

