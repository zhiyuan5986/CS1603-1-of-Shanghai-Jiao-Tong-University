#include <iostream>
using namespace std;

int main()
{
    int n, zheng=0, fu=0;
    do {
        cin >> n;
        if (n<0) ++fu;
        if (n>0) ++zheng;
    } while (n != 0);
    cout << zheng << ' ' << fu;
    return 0;
}


