#include <iostream>
using namespace std;

int main()
{
    int n, m, tmp;
    cin >> n >> m;
    if (n<m) {
        tmp = n;
        n = m;
        m = tmp;//保证n更大
    }
    while (n%m != 0) {
        tmp = n;
        n = m;
        m = tmp%m;
    }
    cout << m;
    return 0;
}



